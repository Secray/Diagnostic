<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.asusodm.atd.smmitest"
    android:sharedUserId="android.uid.system">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.BODY_SENSORS" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-feature android:name="android.hardware.camera.front" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.USE_FINGERPRINT" />
    <uses-permission android:name="android.permission.MANAGE_FINGERPRINT" />
    <uses-permission android:name="android.permission.NFC" />

    <application
        android:name="com.wingtech.diagnostic.App"
        android:allowBackup="true"
        android:icon="@drawable/asus_icon_self_diagnostic"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name="com.wingtech.diagnostic.activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <!--<category android:name="android.intent.category.LAUNCHER" />-->
            </intent-filter>
        </activity>

        <activity
            android:name="com.asusodm.atd.smmitest.main.MAIN"
            android:label="@string/app_name"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>

        <activity
            android:name="com.wingtech.diagnostic.activity.RepairActivity"
            android:label="@string/main_bottom_button_repair"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.wingtech.diagnostic.activity.HelpActivity"
            android:label="@string/main_bottom_button_help"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.wingtech.diagnostic.activity.AboutActivity"
            android:label="@string/menu_test_about"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.wingtech.diagnostic.activity.HotlineServiceActivity"
            android:label="@string/help_list_item_hotline_service"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.wingtech.diagnostic.activity.SendRepairRequestActivity"
            android:label="@string/repair_request_name"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:windowSoftInputMode="adjustResize" />

        <activity
            android:name="com.wingtech.diagnostic.activity.TestResultActivity"
            android:label="@string/menu_test_result"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.wingtech.diagnostic.activity.TouchActivity"
            android:label="@string/touch_title"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.wingtech.diagnostic.activity.SingleTestListActivity"
            android:label="@string/main_bottom_button_single_test"
            android:launchMode="singleTask"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.wingtech.diagnostic.activity.SingleTestActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.wingtech.diagnostic.activity.TouchTestActivity"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.wingtech.diagnostic.activity.CameraTestActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.DisplayActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.ProximityActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.KeypadActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.HeadsetKeyActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.LightSensorActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.NfcActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.ChargingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.RecieverActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.HeadsetActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.SpeakerActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.BoardMicActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.HeadsetMicActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.SecondaryMicActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.TestAllActivity"
            android:configChanges="orientation|screenSize|keyboardHidden|keyboard|screenLayout|mcc|mnc"
            android:screenOrientation="portrait" />

        <activity
            android:name="com.wingtech.diagnostic.activity.CameraFlashActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.SingleTestingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.GSensorTestActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.MultiTouchTestingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.VibratorTestingActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.CallActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.CellularNetworkActivity"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.wingtech.diagnostic.activity.VirtualKeyActivity"
            android:screenOrientation="portrait" />

        <service android:name="com.wingtech.diagnostic.service.TemperatureService" />
        <service android:name="com.goodix.service.FingerprintManagerService">
            <intent-filter>
                <!-- AIDL完整路径名。必须指明，客户端能够通过AIDL类名查找到它的实现类 -->
                <action android:name="com.goodix.service.FingerprintManagerService" />
            </intent-filter>
        </service>
    </application>

</manifest>
