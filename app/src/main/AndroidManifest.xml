<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wingtech.diagnostic">

    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE"/>
    <uses-permission android:name="android.permission.CHANGE_NETWORK_STATE"/>
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION"/>
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION"/>
    <uses-permission android:name="android.permission.BODY_SENSORS"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.VIBRATE"/>
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />
    <uses-feature android:name="android.hardware.camera.front"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.RECORD_AUDIO" />

    <uses-permission android:name="android.permission.CHANGE_WIFI_STATE" />

    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity android:name="com.wingtech.diagnostic.activity.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:label="@string/main_bottom_button_repair"
            android:screenOrientation="portrait"
            android:name="com.wingtech.diagnostic.activity.RepairActivity"
            android:launchMode="singleTask"/>

        <activity
            android:label="@string/main_bottom_button_help"
            android:screenOrientation="portrait"
            android:name="com.wingtech.diagnostic.activity.HelpActivity"
            android:launchMode="singleTask"/>

        <activity android:name="com.wingtech.diagnostic.activity.AboutActivity"
            android:label="@string/menu_test_about"
            android:screenOrientation="portrait"
            android:launchMode="singleTask"/>

        <activity android:name="com.wingtech.diagnostic.activity.HotlineServiceActivity"
            android:label="@string/help_list_item_hotline_service"
            android:screenOrientation="portrait"
            android:launchMode="singleTask"/>

        <activity android:name="com.wingtech.diagnostic.activity.SendRepairRequestActivity"
            android:windowSoftInputMode="adjustResize"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:label="@string/repair_request_name"/>

        <activity android:name="com.wingtech.diagnostic.activity.TestResultActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:label="@string/menu_test_result"/>

        <activity android:name="com.wingtech.diagnostic.activity.TouchActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:label="@string/touch_title"/>

        <activity android:name="com.wingtech.diagnostic.activity.SingleTestListActivity"
            android:launchMode="singleTask"
            android:screenOrientation="portrait"
            android:label="@string/main_bottom_button_single_test"/>

        <activity android:name="com.wingtech.diagnostic.activity.SingleTestActivity"
            android:screenOrientation="portrait"/>

        <activity android:name="com.wingtech.diagnostic.activity.TouchTestActivity"
            android:screenOrientation="portrait"/>

        <activity android:name="com.wingtech.diagnostic.activity.CameraTestActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.wingtech.diagnostic.activity.DisplayActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.wingtech.diagnostic.activity.ProximityActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.wingtech.diagnostic.activity.KeypadActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.wingtech.diagnostic.activity.HeadsetKeyActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.wingtech.diagnostic.activity.LightSensorActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.wingtech.diagnostic.activity.NfcActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.wingtech.diagnostic.activity.WireChargActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.wingtech.diagnostic.activity.RecieverActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.wingtech.diagnostic.activity.HeadsetActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.wingtech.diagnostic.activity.SpeakerActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.wingtech.diagnostic.activity.BoardMicActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.wingtech.diagnostic.activity.SecondaryMicActivity"
            android:screenOrientation="portrait"/>
        <activity android:name="com.wingtech.diagnostic.activity.HeadsetMicActivity"
            android:screenOrientation="portrait"/>

        <activity android:name="com.wingtech.diagnostic.activity.TestAllActivity"
            android:screenOrientation="portrait"/>

        <activity android:name=".activity.CameraFlashActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".activity.SingleTestingActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".activity.GSensorTestActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".activity.MultiTouchTestingActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".activity.VibratorTestingActivity"
            android:screenOrientation="portrait"/>

    </application>

</manifest>
